@base <http://example.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lambdaber:dataset-loosenin-bioenergy a lambdaber:Dataset ;
    dcterms:title "Fungal Loosenins for Bioenergy Applications - Multi-facility Structural Biology Investigation" ;
    lambdaber:studies lambdaber:study-loosenin-2025 .

lambdaber:df-cryoem-001 a lambdaber:DataFile ;
    dcterms:title "CryoEM particle stack" ;
    lambdaber:data_type "particles" ;
    lambdaber:file_format "star" ;
    lambdaber:file_name "loosenin_cellulose.star" ;
    lambdaber:file_path "/data/cryoem/2025/loosenin/" ;
    lambdaber:file_size_bytes 125000000000 .

lambdaber:df-map-001 a lambdaber:DataFile ;
    dcterms:title "CryoEM density map" ;
    lambdaber:data_type "volume" ;
    lambdaber:file_format "mrc" ;
    lambdaber:file_name "loosenin_cellulose_map.mrc" ;
    lambdaber:file_path "/data/cryoem/2025/loosenin/" ;
    lambdaber:file_size_bytes 512000000 .

lambdaber:df-mx-001 a lambdaber:DataFile ;
    dcterms:title "X-ray diffraction data" ;
    lambdaber:data_type "diffraction" ;
    lambdaber:file_format "mtz" ;
    lambdaber:file_name "loosenin_native.mtz" ;
    lambdaber:file_path "/data/mx/2025/loosenin/" ;
    lambdaber:file_size_bytes 45000000 .

lambdaber:df-pdb-001 a lambdaber:DataFile ;
    dcterms:title "Refined crystal structure" ;
    lambdaber:data_type "model" ;
    lambdaber:file_format "pdb" ;
    lambdaber:file_name "loosenin_refined.pdb" ;
    lambdaber:file_path "/data/mx/2025/loosenin/" ;
    lambdaber:file_size_bytes 800000 .

lambdaber:df-sans-001 a lambdaber:DataFile ;
    dcterms:title "SANS complex data" ;
    lambdaber:data_type "scattering" ;
    lambdaber:file_format "ascii" ;
    lambdaber:file_name "biosans_complex.dat" ;
    lambdaber:file_path "/data/sans/2025/loosenin/" ;
    lambdaber:file_size_bytes 1800000 .

lambdaber:df-saxs-001 a lambdaber:DataFile ;
    dcterms:title "SAXS pH series data" ;
    lambdaber:data_type "scattering" ;
    lambdaber:file_format "ascii" ;
    lambdaber:file_name "loosenin_ph_series.dat" ;
    lambdaber:file_path "/data/saxs/2025/loosenin/" ;
    lambdaber:file_size_bytes 2500000 .

lambdaber:img-sans-001 a lambdaber:Image ;
    dcterms:title "SANS model fit" ;
    lambdaber:acquisition_date "2025-02-25" ;
    lambdaber:file_name "sans_model_fit.png" .

lambdaber:img-saxs-001 a lambdaber:Image ;
    dcterms:title "SAXS Guinier plots" ;
    lambdaber:acquisition_date "2025-02-12" ;
    lambdaber:file_name "saxs_guinier_plots.png" .

lambdaber:prep-cryoem-001 a lambdaber:SamplePreparation ;
    dcterms:title "CryoEM grid preparation of complex" ;
    lambdaber:preparation_type "cryo_em" ;
    lambdaber:sample_id "lambdaber:sample-LOOS-004" .

lambdaber:prep-crystal-001 a lambdaber:SamplePreparation ;
    dcterms:title "Crystal preparation for X-ray diffraction" ;
    lambdaber:preparation_type "xray_crystallography" ;
    lambdaber:sample_id "lambdaber:sample-LOOS-001" .

lambdaber:prep-sans-001 a lambdaber:SamplePreparation ;
    dcterms:title "SANS contrast matching preparation" ;
    lambdaber:preparation_type "sans" ;
    lambdaber:sample_id "lambdaber:sample-LOOS-001" .

lambdaber:prep-saxs-001 a lambdaber:SamplePreparation ;
    dcterms:title "SAXS sample preparation" ;
    lambdaber:preparation_type "saxs" ;
    lambdaber:sample_id "lambdaber:sample-LOOS-001" .

lambdaber:sample-LOOS-001 a lambdaber:Sample ;
    dcterms:title "PcaLOOL12 wild-type protein" ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:additives "1mM DTT",
                "5% glycerol" ;
            lambdaber:components "150mM NaCl",
                "50mM MES buffer" ;
            lambdaber:ph "6.0"^^xsd:float ] ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:ligands "calcium ions" ;
            lambdaber:modifications "N-glycosylation at positions 45, 78, 156" ;
            lambdaber:sequences """MKVLSFAIAAALALATAPAAAEDGTVVDCTNYQGRCGSQCGPTGLCCSKYGWCGNTDHYCGDGCQSQCP YGGQPGTPGGQPGTPGGQPGTPGGQPGTPGGQPGTPGGNSYTTTTTSSSGGGGSGGSGGSTTNAGAGPS TPSAPSAPSAPAAPSAPSTPSAPSTPSAPSTPSAPSTPSAPGNYTLQCDPATGWLSCNGGPCNPGSCCS QFGYCGSGAAYCGAGCQGGPCQPPC
""" ] ;
    lambdaber:sample_code "LOOS-001" ;
    lambdaber:sample_type "protein" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:atmosphere "argon-purged" ;
            lambdaber:duration "7 days" ;
            lambdaber:temperature "4.0"^^xsd:float ;
            lambdaber:temperature_unit "celsius" ] .

lambdaber:sample-LOOS-002 a lambdaber:Sample ;
    dcterms:title "PcaLOOL12 pH stress series" ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:components "100mM NaCl",
                "Universal buffer system" ;
            lambdaber:ph "5.0"^^xsd:float ] ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:modifications "N-glycosylation" ;
            lambdaber:sequences "Same as LOOS-001" ] ;
    lambdaber:sample_code "LOOS-002" ;
    lambdaber:sample_type "protein" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:temperature "4.0"^^xsd:float ;
            lambdaber:temperature_unit "celsius" ] .

lambdaber:sample-LOOS-003 a lambdaber:Sample ;
    dcterms:title "TrLOOL1 from Trichoderma reesei" ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:components "200mM NaCl",
                "50mM sodium acetate" ;
            lambdaber:ph "5.5"^^xsd:float ] ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:ligands "calcium ions" ;
            lambdaber:sequences "TrLOOL1 sequence - homolog from T. reesei" ] ;
    lambdaber:sample_code "LOOS-003" ;
    lambdaber:sample_type "protein" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:temperature "4.0"^^xsd:float ;
            lambdaber:temperature_unit "celsius" ] .

lambdaber:sample-LOOS-004 a lambdaber:Sample ;
    dcterms:title "PcaLOOL12-cellulose complex" ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:additives "10mg/mL Avicel cellulose" ;
            lambdaber:components "150mM NaCl",
                "50mM MES buffer" ;
            lambdaber:ph "6.0"^^xsd:float ] ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:ligands "microcrystalline cellulose (Avicel PH-101)" ;
            lambdaber:sequences "PcaLOOL12 protein sequence" ] ;
    lambdaber:sample_code "LOOS-004" ;
    lambdaber:sample_type "complex" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:temperature "4.0"^^xsd:float ;
            lambdaber:temperature_unit "celsius" ] .

lambdaber:study-loosenin-2025 a lambdaber:Study ;
    dcterms:title "Structural and functional characterization of fungal loosenins" ;
    lambdaber:data_files lambdaber:df-cryoem-001,
        lambdaber:df-map-001,
        lambdaber:df-mx-001,
        lambdaber:df-pdb-001,
        lambdaber:df-sans-001,
        lambdaber:df-saxs-001 ;
    lambdaber:images lambdaber:img-sans-001,
        lambdaber:img-saxs-001 ;
    lambdaber:sample_preparations lambdaber:prep-cryoem-001,
        lambdaber:prep-crystal-001,
        lambdaber:prep-sans-001,
        lambdaber:prep-saxs-001 ;
    lambdaber:samples lambdaber:sample-LOOS-001,
        lambdaber:sample-LOOS-002,
        lambdaber:sample-LOOS-003,
        lambdaber:sample-LOOS-004 ;
    lambdaber:workflow_runs lambdaber:wf-cryoem-001,
        lambdaber:wf-integrative-001,
        lambdaber:wf-mx-001,
        lambdaber:wf-saxs-001 .

lambdaber:wf-cryoem-001 a lambdaber:WorkflowRun ;
    dcterms:title "Single particle reconstruction" ;
    lambdaber:experiment_id "lambdaber:exp-cryoem-001" ;
    lambdaber:processing_level 3 ;
    lambdaber:software_name "RELION" ;
    lambdaber:software_version "4.0" ;
    lambdaber:workflow_code "CryoEM-SPA" ;
    lambdaber:workflow_type "refinement" .

lambdaber:wf-integrative-001 a lambdaber:WorkflowRun ;
    dcterms:title "Integrative modeling" ;
    lambdaber:experiment_id "lambdaber:exp-saxs-001" ;
    lambdaber:processing_level 2 ;
    lambdaber:software_name "IMP" ;
    lambdaber:software_version "2.18" ;
    lambdaber:workflow_code "IMP-modeling" ;
    lambdaber:workflow_type "model_building" .

lambdaber:wf-mx-001 a lambdaber:WorkflowRun ;
    dcterms:title "Crystal structure determination" ;
    lambdaber:experiment_id "lambdaber:exp-mx-001" ;
    lambdaber:processing_level 3 ;
    lambdaber:software_name "PHENIX" ;
    lambdaber:software_version "1.20.1" ;
    lambdaber:workflow_code "MX-structure" ;
    lambdaber:workflow_type "phasing" .

lambdaber:wf-saxs-001 a lambdaber:WorkflowRun ;
    dcterms:title "SAXS pH series analysis" ;
    lambdaber:experiment_id "lambdaber:exp-saxs-001" ;
    lambdaber:processing_level 2 ;
    lambdaber:software_name "ATSAS" ;
    lambdaber:software_version "3.2.1" ;
    lambdaber:workflow_code "SAXS-pH-analysis" ;
    lambdaber:workflow_type "saxs_analysis" .

