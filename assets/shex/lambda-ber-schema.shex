<AggregatedProteinView> CLOSED {
    (  $<AggregatedProteinView_tes> (  &<NamedThing_tes> ;
          rdf:type [ <NamedThing> ] ? ;
          <uniprot_id> @linkml:String ;
          <protein_name> @linkml:String ;
          <organism> @linkml:String ? ;
          <organism_id> @linkml:Integer ? ;
          <pdb_entries> @linkml:String * ;
          <functional_sites> @<FunctionalSite> * ;
          <structural_features> @<StructuralFeature> * ;
          <protein_interactions> @<ProteinProteinInteraction> * ;
          <ligand_interactions> @<LigandInteraction> * ;
          <mutations> @<MutationEffect> * ;
          <ptms> @<PostTranslationalModification> * ;
          <biophysical_properties> @<BiophysicalProperty> * ;
          <conformational_ensemble> @<ConformationalEnsemble> ? ;
          <evolutionary_conservation> @<EvolutionaryConservation> ? ;
          <cross_references> @<DatabaseCrossReference> *
       ) ;
       rdf:type [ <AggregatedProteinView> ]
    )
}

<AttributeGroup>  (
    @<BiophysicalProperty> OR @<BufferComposition> OR @<ComputeResources> OR @<ConformationalState> OR @<DataCollectionStrategy> OR
    @<DatabaseCrossReference> OR @<ExperimentalConditions> OR @<ImageFeature> OR @<LigandInteraction> OR @<MolecularComposition> OR
    @<QualityMetrics> OR @<StorageConditions> OR @<TechniqueSpecificPreparation>
)

<AttributeGroup_struct> {
    (  $<AttributeGroup_tes> <description> @linkml:String ? ;
       rdf:type [ <AttributeGroup> ] ?
    )
}

