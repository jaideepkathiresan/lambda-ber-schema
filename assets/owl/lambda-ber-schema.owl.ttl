            owl:onProperty lambdaber:pixel_size_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:float ;
            owl:onProperty lambdaber:pixel_size_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:intersectionOf ( [ a rdfs:Datatype ;
                                owl:unionOf ( [ a rdfs:Datatype ;
                                            owl:intersectionOf ( [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:minInclusive 120 ] ) ] [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:maxInclusive 120 ] ) ] ) ] [ a rdfs:Datatype ;
                                            owl:intersectionOf ( [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:minInclusive 200 ] ) ] [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:maxInclusive 200 ] ) ] ) ] [ a rdfs:Datatype ;
                                            owl:intersectionOf ( [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:minInclusive 300 ] ) ] [ a rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:maxInclusive 300 ] ) ] ) ] ) ] xsd:integer ) ] ;
            owl:onProperty lambdaber:accelerating_voltage ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:float ;
            owl:onProperty lambdaber:pixel_size_min ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty lambdaber:detector_dimensions ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty lambdaber:autoloader_capacity ],
