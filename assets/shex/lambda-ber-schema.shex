             <https://w3id.org/lambda-ber-schema/BiophysicalPropertyEnum#sedimentation_coefficient>
             <https://w3id.org/lambda-ber-schema/BiophysicalPropertyEnum#radius_of_gyration>
             <https://w3id.org/lambda-ber-schema/BiophysicalPropertyEnum#hydrodynamic_radius> ] ;
          <value> @linkml:Float ;
          <unit> @linkml:String ;
          <error> @linkml:Float ? ;
          <measurement_conditions> @linkml:String ? ;
