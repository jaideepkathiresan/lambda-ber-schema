    (  $<XRayPreparation_tes> (  &<TechniqueSpecificPreparation_tes> ;
          rdf:type [ <TechniqueSpecificPreparation> ] ? ;
          <crystallization_method> [ <https://w3id.org/lambda-ber-schema/CrystallizationMethodEnum#vapor_diffusion_hanging>
             <https://w3id.org/lambda-ber-schema/CrystallizationMethodEnum#vapor_diffusion_sitting>
             <https://w3id.org/lambda-ber-schema/CrystallizationMethodEnum#microbatch>
             <https://w3id.org/lambda-ber-schema/CrystallizationMethodEnum#dialysis>
             <https://w3id.org/lambda-ber-schema/CrystallizationMethodEnum#free_interface_diffusion> ] ? ;
          <crystallization_conditions> @linkml:String ? ;
          <crystal_size> @linkml:String ? ;
          <cryoprotectant> @linkml:String ? ;
          <cryoprotectant_concentration> @linkml:Float ? ;
          <mounting_method> @linkml:String ? ;
          <flash_cooling_method> @linkml:String ?
       ) ;
       rdf:type [ <XRayPreparation> ] ?
    )
}

<XRFImage> CLOSED {
    (  $<XRFImage_tes> (  &<Image2D_tes> ;
          rdf:type [ <Image2D> ] ? ;
          <beam_energy> @linkml:Float ? ;
          <beam_size> @linkml:Float ? ;
          <dwell_time> @linkml:Float ? ;
          <elements_measured> @linkml:String * ;
          <source_type> [ <https://w3id.org/lambda-ber-schema/XRaySourceTypeEnum#synchrotron>
             <https://w3id.org/lambda-ber-schema/XRaySourceTypeEnum#rotating_anode>
             <https://w3id.org/lambda-ber-schema/XRaySourceTypeEnum#microfocus>
             <https://w3id.org/lambda-ber-schema/XRaySourceTypeEnum#metal_jet> ] ? ;
          <detector_type> @linkml:String ? ;
          <flux> @linkml:Float ? ;
          <calibration_standard> @linkml:String ?
       ) ;
       rdf:type [ <XRFImage> ]
    )
}
