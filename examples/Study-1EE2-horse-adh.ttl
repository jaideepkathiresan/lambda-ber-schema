@base <http://example.org/> .
@prefix @base: <http://example.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lambdaber:study_1EE2_horse_adh a lambdaber:Study ;
    dcterms:title "Crystal Structure of Horse Liver Alcohol Dehydrogenase at 1.54 A Resolution" ;
    lambdaber:data_files lambdaber:file_1EE2_coords,
        lambdaber:file_1EE2_sf ;
    lambdaber:description "The structure of steroid-active alcohol dehydrogenase from Equus caballus (horse) liver, complexed with NAD+ and cholic acid" ;
    lambdaber:images lambdaber:img_1EE2_diffraction_001 ;
    lambdaber:instrument_runs lambdaber:exp_1EE2_xray_001 ;
    lambdaber:sample_preparations lambdaber:prep_1EE2_001 ;
    lambdaber:samples lambdaber:sample_1EE2_001 ;
    lambdaber:workflow_runs lambdaber:wf_1EE2_refinement_001 .

lambdaber:exp_1EE2_xray_001 a lambdaber:ExperimentRun ;
    dcterms:title "X-ray Diffraction Data Collection" ;
    lambdaber:experiment_code "1EE2_XRAY_001" ;
    lambdaber:experiment_date "1998-11-23" ;
    lambdaber:experimental_conditions [ a lambdaber:ExperimentalConditions ;
            lambdaber:temperature "100.0"^^xsd:float ] ;
    lambdaber:instrument_id <DESY_BW7B> ;
    lambdaber:quality_metrics [ a lambdaber:QualityMetrics ;
            lambdaber:completeness "95.0"^^xsd:float ;
            lambdaber:resolution "1.54"^^xsd:float ] ;
    lambdaber:sample_id "lambdaber:sample_1EE2_001" ;
    lambdaber:technique "xray_crystallography" .

lambdaber:file_1EE2_coords a lambdaber:DataFile ;
    lambdaber:creation_date "2000-01-30" ;
    lambdaber:data_type "model" ;
    lambdaber:file_format "pdb" ;
    lambdaber:file_name "1EE2.pdb" .

lambdaber:file_1EE2_sf a lambdaber:DataFile ;
    lambdaber:creation_date "2000-01-30" ;
    lambdaber:data_type "diffraction" ;
    lambdaber:file_format "mmcif" ;
    lambdaber:file_name "1EE2_sf.cif" .

lambdaber:img_1EE2_diffraction_001 a lambdaber:Image ;
    lambdaber:acquisition_date "1998-11-23" ;
    lambdaber:file_name "1ee2_diffraction_pattern.img" ;
    lambdaber:pixel_size "0.15"^^xsd:float .

lambdaber:prep_1EE2_001 a lambdaber:SamplePreparation ;
    lambdaber:description "Vapor diffusion sitting drop with PEG400/PEG8000, TES/KOH pH 7.0, 277K, NAD+ and Cholic Acid" ;
    lambdaber:preparation_date "1998-11-01" ;
    lambdaber:preparation_type "xray_crystallography" ;
    lambdaber:sample_id "lambdaber:sample_1EE2_001" .

lambdaber:sample_1EE2_001 a lambdaber:Sample ;
    dcterms:title "Horse Liver Alcohol Dehydrogenase" ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:components "TES/KOH" ;
            lambdaber:ph "7.0"^^xsd:float ] ;
    lambdaber:concentration "10.0"^^xsd:float ;
    lambdaber:concentration_unit "mg_per_ml" ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:ligands "2x Zinc Ion",
                "Alcohol Dehydrogenase (373 residues)",
                "Cholic Acid",
                "NAD+" ] ;
    lambdaber:molecular_weight "80.0"^^xsd:float ;
    lambdaber:sample_code "1EE2_SAMPLE_001" ;
    lambdaber:sample_type "complex" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:temperature "277.0"^^xsd:float ;
            lambdaber:temperature_unit "kelvin" ] .

lambdaber:wf_1EE2_refinement_001 a lambdaber:WorkflowRun ;
    dcterms:title "Structure Refinement" ;
    lambdaber:experiment_id "lambdaber:exp_1EE2_xray_001" ;
    lambdaber:software_name "CNS" ;
    lambdaber:software_version "1.0" ;
    lambdaber:workflow_code "1EE2_REFINEMENT_001" ;
    lambdaber:workflow_type "refinement" .

