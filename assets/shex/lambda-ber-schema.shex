<Study> CLOSED {
    (  $<Study_tes> (  &<NamedThing_tes> ;
          rdf:type [ <NamedThing> ] ? ;
          <samples> @<Sample> * ;
          <sample_preparations> @<SamplePreparation> * ;
          <instrument_runs> @<ExperimentRun> * ;
          <workflow_runs> @<WorkflowRun> * ;
          <data_files> @<DataFile> * ;
          <images> @<Image> *
       ) ;
       rdf:type [ <Study> ]
    )
}

<TechniqueSpecificPreparation>  (
    @<CryoEMPreparation> OR @<SAXSPreparation> OR @<XRayPreparation>
)

<TechniqueSpecificPreparation_struct> {
    (  $<TechniqueSpecificPreparation_tes> (  &<AttributeGroup_tes> ;
          rdf:type [ <AttributeGroup> ] ?
       ) ;
