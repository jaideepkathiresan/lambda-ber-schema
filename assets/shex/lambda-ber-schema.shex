          <sample_preparations> @<SamplePreparation> * ;
          <instrument_runs> @<ExperimentRun> * ;
          <workflow_runs> @<WorkflowRun> * ;
          <data_files> @<DataFile> * ;
          <images> @<Image> * ;
          <aggregated_protein_views> @<AggregatedProteinView> *
       ) ;
       rdf:type [ <Study> ]
    )
}

<TechniqueSpecificPreparation>  (
