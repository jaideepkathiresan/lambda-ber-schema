@base <http://example.org/> .
@prefix NCBITaxon: <http://purl.obolibrary.org/obo/NCBITaxon_> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.org/kibler2024/sample/hetBGL03-15-18> a lambdaber:Sample ;
    lambdaber:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:components "25 mM Tris",
                "300 mM NaCl" ;
            lambdaber:ph "8.0"^^xsd:float ] ;
    lambdaber:concentration "2.5"^^xsd:float ;
    lambdaber:concentration_unit "mg_per_ml" ;
    lambdaber:description "Pseudosymmetric hetero-hexamer with three distinct chains" ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:modifications "6xHis tag (removed after purification)",
                "TEV protease cleavage site" ;
            lambdaber:sequences "MDFYTLPTSHPA...",
                "MDFYTLPTSHPV...",
                "MDFYTLPTSNPV..." ] ;
    lambdaber:molecular_weight "182.5"^^xsd:float ;
    lambdaber:organism NCBITaxon:562 ;
    lambdaber:sample_code "hetBGL03-15-18" ;
    lambdaber:sample_type "complex" ;
    lambdaber:storage_conditions [ a lambdaber:StorageConditions ;
            lambdaber:duration "< 1 week before measurement" ;
            lambdaber:temperature "4.0"^^xsd:float ;
            lambdaber:temperature_unit "celsius" ] .

