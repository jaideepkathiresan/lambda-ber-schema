lambdaber:AttributeGroup a sh:NodeShape ;
    sh:closed false ;
    sh:description "A grouping of related data attributes that form a logical unit" ;
    sh:ignoredProperties ( lambdaber:cell_path_length lambdaber:total_frames lambdaber:crystal_size lambdaber:grid_type lambdaber:pressure lambdaber:concentration_series lambdaber:buffer_matching_protocol lambdaber:blot_force lambdaber:humidity lambdaber:frame_rate lambdaber:flash_cooling_method lambdaber:total_dose lambdaber:vitrification_method lambdaber:completeness lambdaber:exposure_time lambdaber:terms lambdaber:signal_to_noise lambdaber:components lambdaber:temperature lambdaber:r_factor lambdaber:cpu_hours lambdaber:memory_gb lambdaber:ligands lambdaber:chamber_temperature lambdaber:cryoprotectant_concentration rdf:type lambdaber:modifications lambdaber:cryoprotectant lambdaber:collection_mode lambdaber:plasma_treatment lambdaber:sequences lambdaber:beam_energy lambdaber:crystallization_conditions lambdaber:additives lambdaber:i_zero lambdaber:crystallization_method lambdaber:humidity_percentage lambdaber:temperature_unit lambdaber:rg lambdaber:gpu_hours lambdaber:hole_size lambdaber:support_film lambdaber:sample_cell_type lambdaber:mounting_method lambdaber:storage_gb lambdaber:duration lambdaber:blot_time lambdaber:resolution lambdaber:ph lambdaber:atmosphere lambdaber:temperature_control lambdaber:dose_per_frame ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 0 ;
            sh:path lambdaber:description ] ;
    sh:targetClass lambdaber:AttributeGroup .

lambdaber:CryoEMInstrument a sh:NodeShape ;
    sh:closed true ;
    sh:description "Cryo-EM microscope specifications" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Current operational status" ;
            sh:in ( "operational" "maintenance" "offline" "commissioning" ) ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path lambdaber:current_status ],
        [ sh:datatype xsd:boolean ;
            sh:description "Spherical aberration corrector present" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 1 ;
            sh:path lambdaber:cs_corrector ],
        [ sh:datatype xsd:string ;
            sh:description "Date of instrument installation" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path lambdaber:installation_date ],
        [ sh:datatype xsd:string ;
            sh:description "Detector dimensions in pixels (e.g., 4096x4096)" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path lambdaber:detector_dimensions ],
        [ sh:datatype xsd:integer ;
            sh:description "Number of grids the autoloader can hold" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path lambdaber:autoloader_capacity ],
        [ sh:datatype xsd:string ;
            sh:description "Instrument model" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path lambdaber:model ],
        [ sh:datatype xsd:boolean ;
            sh:description "Phase plate available" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 2 ;
            sh:path lambdaber:phase_plate ],
        [ sh:description "Accelerating voltage in kV" ;
            sh:maxCount 1 ;
            sh:or ( [ ] [ ] [ ] ) ;
            sh:order 0 ;
            sh:path lambdaber:accelerating_voltage ],
        [ sh:datatype xsd:string ;
            sh:description "Human-friendly facility or laboratory identifier for the instrument (e.g., 'TITAN-KRIOS-1', 'ALS-12.3.1-SIBYLS', 'RIGAKU-FR-E'). Used for local reference and equipment tracking." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 8 ;
            sh:path lambdaber:instrument_code ],
        [ sh:datatype xsd:float ;
            sh:description "Maximum pixel size in Angstroms per pixel" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path lambdaber:pixel_size_max ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path dcterms:title ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path lambdaber:description ],
        [ sh:datatype xsd:float ;
            sh:description "Minimum pixel size in Angstroms per pixel" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path lambdaber:pixel_size_min ],
        [ sh:description "Type of detector" ;
            sh:in ( "direct_electron" "ccd" "cmos" "hybrid_pixel" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path lambdaber:detector_type ],
        [ sh:datatype xsd:anyURI ;
            sh:description "Globally unique identifier as an IRI or CURIE for machine processing and external references. Used for linking data across systems and semantic web integration." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 13 ;
            sh:path lambdaber:id ],
        [ sh:datatype xsd:string ;
            sh:description "Instrument manufacturer" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path lambdaber:manufacturer ] ;
    sh:targetClass lambdaber:CryoEMInstrument .

lambdaber:CryoEMPreparation a sh:NodeShape ;
    sh:closed true ;
    sh:description "Cryo-EM specific sample preparation" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path lambdaber:description ],
        [ sh:datatype xsd:string ;
            sh:description "Support film type" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
